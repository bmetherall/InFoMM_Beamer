\mode<presentation>

% Requirement
\RequirePackage{tikz}
\RequirePackage{graphicx}

% Settings
\useinnertheme{InFoMM}
\useoutertheme{InFoMM}
\usecolortheme{InFoMM}

\setbeamertemplate{navigation symbols}{}
\setbeamertemplate{blocks}[rounded][shadow=true]
\setbeamertemplate{itemize items}[square]
\setbeamertemplate{headline}[InFoMM]
\setbeamertemplate{left bar}{}
\setbeamertemplate{footline}[InFoMM]
\setbeamersize{text margin left=40pt,text margin right=10pt}

\mode<all>